@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":ListStudentCommand" as ListStudentCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("list_student")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("list_student")
activate AddressBookParser

create ListStudentCommand
AddressBookParser -> ListStudentCommand
activate ListStudentCommand

ListStudentCommand --> AddressBookParser :
deactivate ListStudentCommand

AddressBookParser --> LogicManager :
deactivate AddressBookParser

LogicManager -> ListStudentCommand : execute()
activate ListStudentCommand

ListStudentCommand -> Model : updateFilteredStudentList\n(PREDICATE_SHOW_ALL_STUDENTS)
activate Model

Model --> ListStudentCommand
deactivate Model

create CommandResult
ListStudentCommand -> CommandResult
activate CommandResult

CommandResult --> ListStudentCommand
deactivate CommandResult

ListStudentCommand --> LogicManager : result
deactivate ListStudentCommand
'Hidden arrow to position the destroy marker below the end of the activation bar.
ListStudentCommand -[hidden]-> AddressBookParser
destroy ListStudentCommand

[<--LogicManager
deactivate LogicManager
@enduml
