<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="monthlyFeeCardPane" fx:id="monthlyFeeListCardPane" prefHeight="80" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <VBox AnchorPane.leftAnchor="10">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="25.0" />
        </padding>
        <Label fx:id="fee" styleClass="cell_extra_big_label" text="\$fee" wrapText="true" />
    </VBox>
    <Label fx:id="month" styleClass="cell_big_label" text="\$month" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10" />
    <Label fx:id="year" styleClass="cell_big_label" text="\$year" AnchorPane.bottomAnchor="10" AnchorPane.rightAnchor="10" />
</AnchorPane>
